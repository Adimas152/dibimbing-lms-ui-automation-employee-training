<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="RegressionSuite" parallel="methods" thread-count="3">

    <listeners>
        <listener class-name="dibimbing.core.TestListener"/>
    </listeners>

    <test name="RegressionTests">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="dibimbing.tests.employee.EmployeeAddTests"/>
            <class name="dibimbing.tests.employee.EmployeeDeleteTests"/>
            <class name="dibimbing.tests.employee.EmployeeSearchTests"/>
            <class name="dibimbing.tests.employee.EmployeeStatusTests"/>
            <class name="dibimbing.tests.employee.EmployeeEditTests"/>
            <class name="dibimbing.tests.employee.EmployeeTransferTests"/>
            <class name="dibimbing.tests.employee.EmployeeImportTests"/>
        </classes>
    </test>
</suite>
